<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Guncannon</display-name>
  <servlet>
  	<servlet-name>PlantServlet</servlet-name>
  	<display-name>PlantServlet</display-name>
  	<description>Get plant by id</description>
  	<servlet-class>zhaowe.guncannon.servlet.PlantServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>PlantsServlet</servlet-name>
  	<display-name>PlantsServlet</display-name>
  	<description>Get plants</description>
  	<servlet-class>zhaowe.guncannon.servlet.PlantsServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>PlantServlet</servlet-name>
  	<url-pattern>/plant</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>PlantsServlet</servlet-name>
  	<url-pattern>/plants-list</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
  	<welcome-file>index.html</welcome-file>
  </welcome-file-list>
</web-app>
